<%= form_for(@profile, :html => { :multipart => true, :id => 'profile_form' }) do |f| %>
  <%= render :partial => 'shared/form_errors', :locals => {:target => @profile} %>
  <p>
    Profile type
  </p>
  <%= select_tag "profile[profile_type]", "<option>10</option><option>20</option><option>100</option>".html_safe %>

  <p>
    Avatar
  </p>
  <%= f.file_field :avatar %>

  <%= f.submit %>

<% end %>
